<br/>
<h2 class="text-center"> <%= link_to "Event List", root_path %> </h2>
<br/>

<div class="col-xs-10 col-xs-offset-1">
  <table class ="table-bordered center-table" style="width:100%">
    <tr>
      <td><b></b></td>
      <% @tracks.each do |track| %>
        <td class="tracknames"><h4><%= (track.gsub(" - ", "<br/>---------<br/><b>")+"</b>").html_safe %></h4></td>
      <% end %>
    </tr>
    <% @time_periods.each do |time_block| %>
      <tr>
        <td class="times"><%=time_block.strftime("%H%M")%>-<%=(time_block+5.minutes).strftime("%H%M")%></td>
        <% @tracks.each do |track| %>
          <%= generate_schedule_td(time_block, track).html_safe %>
        <% end %>
      </tr>
    <% end %>
  </table>
</div>

<div class="col-xs-10 col-xs-offset-1">
  <br/>
  <h2 class="text-center"> <%= link_to "Event List", root_path %> </h2>
  <br/>
</div>
